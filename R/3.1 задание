a = c(1,0,4,-2,1,1)
b = c(2,3,4,2,0,-3)
d = c(3,1,3,2,9,5)
e = c(4,0,3,2,0,-5)
f = c(5,7,1,-2,8,3)
g = c(6,4,1,2,0,2)
mat = rbind(a,b,d,e,f,g) 
mat
x <- 0
for (i in 1:ncol(mat))
  if (x < (norm(mat[,i],type = "2")))
    x <- norm(mat[,i],type = "2")
print(x)
5)
№по столбцам
for (i in 1:ncol(mat))
  for (j in 1:ncol(mat))
    if (as.numeric(mat[,i]%*%mat[,j])== 0)
      print(c(i,j))
#по строкам
for (i in 1:nrow(mat))
  for (j in 1:nrow(mat))
    if (as.numeric(mat[i,]%*%mat[j,])== 0)
      print(c(i,j))
